                            android:paddingTop="@dimen/service_top_menu_padding"
                            android:scaleType="fitCenter"
                            android:src ="@mipmap/menu_leisure_button"
                            android:background    ="@color/appBg"/>

                        <ImageButton
                            android:id            = "@+id/menu_moviediscount_button"
                            android:layout_width  = "0dp"
                            android:layout_height = "wrap_content"
                            android:layout_weight = "1"
                            android:paddingBottom = "@dimen/service_top_menu_padding"
                            android:paddingTop    = "@dimen/service_top_menu_padding"
                            android:scaleType     = "fitCenter"
                            android:src           = "@mipmap/menu_movie_button"
                            android:background    = "@color/appBg"/>

                        <ImageButton
                            android:id            = "@+id/menu_support_button"
                            android:layout_width  = "0dp"
                            android:layout_height = "wrap_content"
                            android:layout_weight = "1"
                            android:paddingBottom = "@dimen/service_top_menu_padding"
                            android:paddingTop    = "@dimen/service_top_menu_padding"
                            android:scaleType     = "fitCenter"
                            android:src           = "@mipmap/menu_save_support_button"
                            android:background    = "@color/appBg"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width  ="match_parent"
                        android:layout_height ="match_parent"
                        android:gravity       ="center_horizontal"
                        android:orientation   ="horizontal">

                        <ImageButton
                            android:id            ="@+id/menu_movie_button"
                            android:layout_width  ="0dp"
                            android:layout_height ="wrap_content"
                            android:layout_weight ="1"
                            android:paddingBottom ="@dimen/service_top_menu_padding"
                            android:paddingTop    ="@dimen/service_top_menu_padding"
                            android:scaleType     ="fitCenter"
                            android:src           ="@mipmap/menu_video_button"
                            android:background    ="@color/appBg"/>

                        <ImageButton
                            android:id            ="@+id/menu_magazine_button"
                            android:layout_width  ="0dp"
                            android:layout_height ="wrap_content"
                            android:layout_weight ="1"
                            android:paddingBottom ="@dimen/service_top_menu_padding"
                            android:paddingTop    ="@dimen/service_top_menu_padding"
                            android:scaleType     ="fitCenter"
                            android:src           ="@mipmap/menu_tabho_button"
                            android:background    ="@color/appBg"/>

                        <ImageButton
                            android:id            ="@+id/menu_coupon_button"
                            android:layout_width  ="0dp"
                            android:layout_height ="wrap_content"
                            android:layout_weight ="1"
                            android:paddingBottom ="@dimen/service_top_menu_padding"
                            android:paddingTop    ="@dimen/service_top_menu_padding"
                            android:scaleType     ="fitCenter"
                            android:src           ="@mipmap/menu_coupon_button"
                            android:background    ="@color/appBg"/>

                        <ImageButton
                            android:id            ="@+id/menu_present_button"
                            android:layout_width  ="0dp"
                            android:layout_height ="wrap_content"
                            android:layout_weight ="1"
                            android:paddingBottom ="@dimen/service_top_menu_padding"
                            android:paddingTop    ="@dimen/service_top_menu_padding"
                            android:scaleType     ="fitCenter"
                            android:src           ="@mipmap/menu_present_button"
                            android:background    ="@color/appBg"/>

                    </LinearLayout>

                    <include layout="@layout/component_shadow_menu" /> <!--影-->
                </LinearLayout>
            
