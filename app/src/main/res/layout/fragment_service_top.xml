                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingBottom="@dimen/service_top_menu_padding"
                            android:paddingTop="@dimen/service_top_menu_padding"
                            android:scaleType="fitCenter"
                            android:src="@mipmap/menu_ticket_button"
                            android:background="@color/appBg"
                            />

                        <ImageButton
                            android:id="@+id/menu_leisurediscount_button"
